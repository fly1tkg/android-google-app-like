<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android_google_app_like"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="12"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".navigation_drawer.NavigationDrawerActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".swipe_to_dismiss.SwipeToDismissActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".swipe_to_dismiss.SwipeToDismissWithUndoActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".card_ui.CardUiActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>